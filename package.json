  },
  "dependencies": {
    "babel-core": "^6.1.19",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "html-webpack-plugin": "^2.8.1",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1",
    "webpack-glsl-loader": "^1.0.1",
    "@types/dat.gui": "^0.7.4",
    "@types/gl-matrix": "^2.4.5",
    "dat.gui": "^0.7.6",
    "gl-matrix": "^3.1.0"
  },
